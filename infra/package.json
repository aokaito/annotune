{
  "name": "annotune-infra",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:edge && tsc -p tsconfig.json",
    "build:edge": "cd lambda-edge/ogp && npm install && npx esbuild index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --minify",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --require-approval never"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.151.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "aws-cdk": "^2.151.0",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
