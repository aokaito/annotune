{
  "name": "annotune",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "frontend",
    "backend",
    "infra"
  ],
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:frontend": "npm --prefix frontend run lint",
    "lint:backend": "npm --prefix backend run lint",
    "typecheck": "npm-run-all typecheck:*",
    "typecheck:frontend": "npm --prefix frontend run typecheck",
    "typecheck:backend": "npm --prefix backend run typecheck",
    "build:frontend": "npm --prefix frontend run build",
    "build:backend": "npm --prefix backend run build",
    "synth": "npm --prefix infra run synth"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
